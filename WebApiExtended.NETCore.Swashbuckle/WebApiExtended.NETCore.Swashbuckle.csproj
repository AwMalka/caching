<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>netcoreapp2.1</TargetFramework>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    <AssemblyName>AwPubComp.Caching.WebApiExtended.NETCore.Swashbuckle</AssemblyName>
    <RootNamespace>PubComp.Caching.WebApiExtended.Net.Core.Swashbuckle</RootNamespace>
    <Version>4.2.0-b200204-1727</Version>
    <AssemblyVersion>4.2.0.0</AssemblyVersion>
    <FileVersion>4.1.1.0</FileVersion>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Authors>Danny Varod and Avi Malka</Authors>
    <PackageProjectUrl>https://github.com/pub-comp/caching</PackageProjectUrl>
    <Copyright>Danny Varod</Copyright>
    <RepositoryUrl>https://github.com/AwMalka/caching</RepositoryUrl>
    <PackageTags>AwPubComp</PackageTags>
    <PackageReleaseNotes>Fork for https://github.com/AwMalka/caching for testing purposes</PackageReleaseNotes>
    <Product>Web API caching Swashbuckle extensions</Product>
    <Description>Web API caching Swashbuckle extensions</Description>
    <PackageLicenseFile>License.txt</PackageLicenseFile>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <NoWarn>1701;1702;1591</NoWarn>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
    <NoWarn>1701;1702;1591</NoWarn>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Microsoft.AspNetCore.Mvc" Version="2.1.3" />
    <PackageReference Include="NLog" Version="4.5.10" />
    <PackageReference Include="Swashbuckle.AspNetCore" Version="4.0.1" />
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\Core\Core.csproj" />
  </ItemGroup>
  <ItemGroup>
    <None Include="..\License.txt">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>
  <Target Name="PostBuild" AfterTargets="PostBuildEvent">
    <Exec Command="&quot;$(SolutionDir).NuGetPack\NuGetPack.exe&quot; &quot;$(ProjectPath)&quot; &quot;$(TargetPath)&quot; $(ConfigurationName)" />
  </Target>
</Project>